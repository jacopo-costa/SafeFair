<%- include ('./partials/header.ejs') %>

<div id="ultime-fiere">
    <% for( const fiera of fiere ) { %>
        <h2>
            <%= fiera.nome %>
        </h2>
        <img src="/img/fiere/<%= fiera.nome %>/1.jpg" alt="<%= fiera.nome %>">
        <p>
            <%= fiera.descrizione %>
        </p>
        <a href="/<%= fiera.id %>">Clicca qui</a>
        <% } %>
</div>
<h1>
    <%= news.news %>
</h1>

<div id="news">
    <% for( const notizia of news ) { %>
        <h2>
            <%= notizia.news %>
        </h2>
        <% } %>
</div>

